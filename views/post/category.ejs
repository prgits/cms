<%layout('../layouts/default')%>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Post</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    <h3>Post Categories</h3>
                    <div class="table-responsive">
                        <table class="table table-bordered table-striped">
                            <thead>
                            <tr>
                                <th>
                                    Name
                                </th>
                                <th>
                                    Icon
                                </th>
                                <th>
                                    Parent
                                </th>
                                <th>
                                    Created Time
                                </th>
                                <th>
                                    Action
                                </th>
                            </tr>
                            </thead>
                            <tbody id="tb">
                            <%data.forEach(function (cat) {%>
                                <tr>
                                    <td><%=cat.name%></td>
                                    <td><%=cat.icon%></td>
                                    <td>-</td>
                                    <td>-</td>
                                    <td>
                                        <button type='button' class='btn btn-warning btn-circle' onclick=''>
                                            <i class='fa fa-times'></i>
                                        </button>
                                    </td>
                                </tr>
                            <%});%>
                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</div>
<!-- /#page-wrapper -->